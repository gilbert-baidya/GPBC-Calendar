            border-color: #667eea;
        }

        .about-icon {
            font-size: 3em;
            margin-bottom: 15px;
            filter: grayscale(30%);
            transition: filter 0.3s;
        }

        .about-card:hover .about-icon {
            filter: grayscale(0%);
        }

        .about-card h3 {
            color: #667eea;
            margin-bottom: 12px;
            font-size: 1.4em;
        }

        .about-card p {
            color: #666;
            line-height: 1.6;
            margin-bottom: 15px;
            flex-grow: 1;
        }

        .card-link {
            color: #667eea;
            font-weight: 600;
            font-size: 0.95em;
            transition: all 0.3s;
        }

        .about-card:hover .card-link {
            color: #764ba2;
            transform: translateX(5px);
            display: inline-block;
        }

        .cta-btn:hover {
            transform: translateY(-3px);
            box-shadow: 0 5px 15px rgba(102, 126, 234, 0.3);
        }

